{
  "name": "youtube-shorts-controller",
  "version": "1.0.5",
  "description": "Youtube Shorts Controller",
  "homepage": "https://github.com/thanhvu1982/youtube-shorts-controller",
  "repository": "https://github.com/misa198/youtube-shorts-controller/tree/master",
  "author": "Thanh Vu",
  "license": "MIT",
  "scripts": {
    "pre-commit": "lint-staged",
    "build:firefox": "rm -rf build/firefox && cross-env PLATFORM=firefox webpack --config webpack.config.js",
    "build:chromium": "rm -rf build/chromium && cross-env PLATFORM=chromium webpack --config webpack.config.js",
    "build": "npm run build:chromium && npm run build:firefox",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.197",
    "@types/firefox-webext-browser": "^94.0.1",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "file-loader": "^6.2.0",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^2.6.1",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "prettier-package-json": "^2.7.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "dom-loaded": "^3.0.0",
    "selector-observer": "^2.1.6"
  }
}
